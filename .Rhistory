temp_df[row, "index"] = -(duration + 1) + row
current_hour = temp_df[row, "hour"]
temp_df[row, "census"] = filter_df %>% filter(current_hour < dcord_tc) %>% summarize(count = n())
}
# Save data
census_df = rbind(census_df, temp_df)
}
